{
  "name": "finding-good-v2",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:dashboard": "pnpm --filter @finding-good/dashboard dev",
    "dev:priority": "pnpm --filter @finding-good/priority dev",
    "dev:prove": "pnpm --filter @finding-good/prove dev",
    "dev:predict": "pnpm --filter @finding-good/predict dev",
    "dev:together": "pnpm --filter @finding-good/together dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "persona": "node scripts/insert-persona.js",
    "persona:list": "node scripts/insert-persona.js --list",
    "persona:all": "node scripts/insert-persona.js --all"
  },
  "devDependencies": {
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^16.3.1"
  },
  "packageManager": "pnpm@9.0.0"
}
